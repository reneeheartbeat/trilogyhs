<script>
var imageblock = document.querySelectorAll(".text-with-images .imgscol");
imageblock.forEach(item => {
  var numberOfChildren = item.childElementCount;
  if (numberOfChildren == 1) {
    item.classList.add('singleimage');
  }
  if (numberOfChildren == 2) {
    item.classList.add('twoimages');
  }
  if (numberOfChildren == 3) {
    item.classList.add('threeimages');
  }
  if (numberOfChildren == 4) {
    item.classList.add('fourimages');
  }
  if (numberOfChildren == 5) {
    item.classList.add('fiveimages');
  }
  if (numberOfChildren == 6) {
    item.classList.add('siximages');
  }
  if (numberOfChildren >= 7) {
    item.classList.add('sevenplus');
  }
})
</script>


